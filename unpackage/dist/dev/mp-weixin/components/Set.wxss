
.setBody{
	width: 100vw;
	height: 97vh;
	background-color: #eeeeed;
	padding-top: 3vh;
}
.setBox{
	width: 80vw;
	margin: 0 auto;
}
.setTitle{
	font-size: 45rpx;
	margin: 3vh 0;
	margin-left: 10rpx;
	letter-spacing: 3rpx;
}
.setBg,
.guide,
.feedback{
	background-color: #fff;
	width: 100%;
	height: 95rpx;
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
	-webkit-box-pack: justify;
	-webkit-justify-content: space-between;
	        justify-content: space-between;
	-webkit-box-align: center;
	-webkit-align-items: center;
	        align-items: center;
	border-radius: 5px;
	margin-bottom: 3vh;
}
.iconbackground{
	color: #b7cfb8;
	font-size: 40rpx;
}
.guideIcon{
	width: 45rpx;
	height: 45rpx;
	display: -webkit-inline-box;
	display: -webkit-inline-flex;
	display: inline-flex;
	vertical-align: top;
}
.guideIcon,
.iconbackground{
	margin-right: 30rpx;
}
.leftBox{
	margin-left: 30rpx;
	letter-spacing: 3rpx;
}
.rightBox{
	margin-right: 30rpx;
}

